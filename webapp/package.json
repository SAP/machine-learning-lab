    "setup": "CI=false npm install && npm audit fix && npm run build",
    "start": "REACT_APP_LAB_ENDPOINT=http://localhost:9091/ react-scripts start",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test a --watchAll=false",
    "test-interactive": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js ; npm run lint:css",
    "lint:js": "eslint --ext js,jsx",
    "lint:css": "stylelint 'src/**/*.css'",
    "prettier": "prettier --config .prettierrc --write"
  },
